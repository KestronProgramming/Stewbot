// Stewbot debug configurations. 
// These make it easy to launch differnet files from the VSCode debugger. 
{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Stewbot",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/index.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Commands",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/launchCommands.js"
        }
    ]
}