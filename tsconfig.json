{
  "compilerOptions": {
    // "noImplicitAny": true, // Too Strict for JS code
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "strict": false,
    "target": "es2024",
    "strictNullChecks": true,
  },
  "include": [
    "**/*.js",
  ],
  "exclude": [
    "node_modules"
  ]
}
